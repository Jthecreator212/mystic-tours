{
  "buildCommand": "node scripts/vercel-build.js",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXT_TRACE": "0"
  }
} 